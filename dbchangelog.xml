<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
   <changeSet  author="temp"  id="1">
    <createTable schemaName="public" tableName="Apple">
        <column  name="id"  type="int"/>
	<column  name="name"  type="varchar(10)"/>
    </createTable>
    </changeSet>	
   <changeSet id="1" author="temp">
     <loadData 
            file="users.csv"
            tableName="Apple"
	    separator=";"/>
   </changeSet>
   <changeSet  author="temp1"  id="2">
    <createTable schemaName="public" tableName="Mango">
        <column  name="id"  type="int"/>
	<column  name="name"  type="varchar(10)"/>
    </createTable>
   </changeSet>
    <changeSet id="2" author="temp1">
     <loadData 
            file="users.csv"
            tableName="Mango"
	    separator=";"/>
    </changeSet>
   <changeSet  author="temp2"  id="3">
    <createTable schemaName="public" tableName="Kiwi">
        <column  name="id"  type="int"/>
	<column  name="name"  type="varchar(10)"/>
    </createTable>
    </changeSet>
   <changeSet id="3" author="temp2">
     <loadData 
            file="users.csv"
            tableName="Kiwi"
	    separator=";"/>
   </changeSet>
</databaseChangeLog>
