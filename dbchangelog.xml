<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
   <changeSet author="man" id="7">
    <createTable schemaName="public" tableName="vehicle">
        <column  name="brand"  type="varchar(12)"/>
	<column  name="car"  type="varchar(10)"/>
    </createTable>
    </changeSet>	
   <changeSet id="7" author="man">
     <loadData 
            file="users.csv"
            tableName="vehicle"
	    separator=";"/>
   </changeSet>
   <changeSet  author="man" id="8">
    <createTable schemaName="public" tableName="motor">
        <column  name="brand"  type="varchar(12)"/>
	<column  name="car"  type="varchar(10)"/>
    </createTable>
   </changeSet>
    <changeSet id="8" author="man">
     <loadData 
            file="users.csv"
            tableName="motor"
	    separator=";"/>
    </changeSet>
   <changeSet  author="man" id="9">
    <createTable schemaName="public" tableName="machine">
        <column  name="brand"  type="varchar(12)"/>
	<column  name="car"  type="varchar(10)"/>
    </createTable>
    </changeSet>
   <changeSet id="9" author="man">
     <loadData 
            file="users.csv"
            tableName="machine"
	    separator=";"/>
   </changeSet>
</databaseChangeLog>


